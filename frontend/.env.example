# Environment Configuration for Life Changing Journey Frontend
# Copy this file to .env.local and update the values for your environment

# API Base URL - Change this for different deployment environments
# Development
NEXT_PUBLIC_API_URL=http://localhost:8000

# Production (Update with your actual production URL)
# NEXT_PUBLIC_API_URL=https://your-production-api.com

# Staging (Update with your actual staging URL)
# NEXT_PUBLIC_API_URL=https://your-staging-api.com

# Google OAuth (if using Google login)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id

# Other environment-specific configurations
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_APP_VERSION=1.0.0
